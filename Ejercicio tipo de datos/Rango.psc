Algoritmo Rango
    
    Definir Numero Como Real;
    
    Escribir "Ingrese un número:";
    Leer Numero;
    
    Si (Numero >= 1 Y Numero <= 100) Entonces
        
        Escribir "El número ", Numero, " está entre 1 y 100. Resultado: Verdadero";
        
    SiNo
        
        Escribir "El número ", Numero, " NO está entre 1 y 100. Resultado: Falso";
        
    FinSi
    
FinAlgoritmo